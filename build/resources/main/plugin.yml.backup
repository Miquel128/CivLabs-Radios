name: CivLabsRadios
main: com.civlabs.radios.CivLabsRadiosPlugin
version: 1.0.3
api-version: "1.21"
description: Radio blocks with SVC positional audio and server-wide frequencies
authors: [ CivLabs ]
depend: [ voicechat ]
permissions:
  civlabs.radio.craft:
    default: true
  civlabs.radio.place:
    default: true
  civlabs.radio.tune:
    default: true
  civlabs.radio.enable:
    default: true
  civlabs.radio.admin:
    default: op
